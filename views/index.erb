<h3>Последние посты</h3>



<% @result.each do |post| %>
<div class = "panel panel-default">
   <div class = "panel-heading">
      <h3 class = "panel-title">
         <p class="text-right"><kbd><%= post['created_date'] %></kbd></p>
      </h3>
   </div>
   
   <div class = "panel-body">
      <p class="text-left"><%= post['content'] %></p>
   </div>
</div>
<p></p>
<% end %>